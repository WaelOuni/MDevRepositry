<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tn.calculator.squeezer.calculatormaterialdesign" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Calculator"
            android:label="@string/title_activity_calculator">

        </activity>
        <activity
            android:name=".MyNavigationDrawer"
            android:label="Test Navigation drawer"
            android:theme="@style/MyNavigationDrawerTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".section1"
            android:label="@string/title_activity_section1"></activity>
        <activity
            android:name=".section3"
            android:label="@string/title_activity_section3"></activity>
        <activity
            android:name=".help"
            android:label="@string/title_activity_help">
        </activity>
    </application>

</manifest>
